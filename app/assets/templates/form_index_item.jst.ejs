<td>
  <a class="small-button" href="#/forms/<%= form.escape("id") %>/edit"><%= form.escape("title") %></a>
</td>
<td class="created_at_column">
  <span class="link-action" title="<%= form.escape("exact_time") %>"><%= form.escape("time_ago") %></span>
</td>
<td class="actions">
  <button class="delete small-button danger-button">Delete</button>
  <% if (form.get('slug')) { %>
    <a class="link-action" href="/<%= form.get('slug') %>">View</a>
    <a class="link-action" href="#/forms/<%= form.escape('slug') %>/submissions">
      Submissions (<%= submissionCount < 99 ? submissionCount : '99+' %>)
    </a>
  <% } %>
</td>
