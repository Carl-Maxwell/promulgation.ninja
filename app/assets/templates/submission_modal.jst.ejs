<div class="modal-wrapper close-modal"></div>
<div class="modal">
  <div class="header">
    <h2 class="heading">Submission for ???</h2>
  </div>
  <div class="modal-body">
    <table class="data-table">
      <% model.submissionFields().forEach(function(field, i) { %>
        <tr>
          <td><%= fields.at(i).escape('label') %></td>
          <td><%= field.escape("value") %></td>
        </tr>
      <% }); %>
    </table>
  </div>
  <div class="bottomness">
    <button class="good-button close-modal">ok</button>
  </div>
</div>
